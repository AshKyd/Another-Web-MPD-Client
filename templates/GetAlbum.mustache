<h2>Tracks</h2>
<table>
	<thead>
		<tr>
			<th><abbr title="Track Number">#</abbr></th>
			<th>Title</th>
			<th>Time</th>
			<th>Date</th>
		</tr>
	</thead>
	<tbody>
	{{#tracks}}
		<tr>
			<td>{{Track}}</td>
			<td>{{Title}}</td>
			<td>{{Time}}</td>
			<td>{{Date}}</td>
		</tr>

{{/tracks}}
	</tbody>
</table>
